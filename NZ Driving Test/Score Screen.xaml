<Window x:Class="Score_Screen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NZ_Driving_Test"
        mc:Ignorable="d"
        Title="Score Screen" Height="300" Width="300.887" Loaded="Window_Loaded">
    <Grid>
        <Label x:Name="lblName" Content="Hi 'name'" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="81,32,0,0" FontSize="18.667"/>
        <Label x:Name="lblYourScore" Content="Your Score" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="81,87,0,0" FontSize="18.667"/>
        <Label x:Name="lblScore" Content="35 / 35" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="81,144,0,0" FontSize="18.667" FontWeight="Bold"/>
        <Label x:Name="lblPassFail" Content="You've Passed!" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="81,201,0,0" FontSize="18.667" FontWeight="Bold" FontStyle="Italic"/>

    </Grid>
</Window>
